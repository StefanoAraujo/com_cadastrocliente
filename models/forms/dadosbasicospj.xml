		<field name="tipoCliente_id" type="cliente" class="validate-tipoCliente_id  input-block-level" filter="tipoCliente_id" label="COM_CADASTROCLIENTE_LABEL_FORM_TIPOCLIENTE" validate="tipoCliente_id"/>
